  "bugs": {
    "url": "https://github.com/luiseduardogfranca/event-scheduling/issues"
  },
  "homepage": "https://github.com/luiseduardogfranca/event-scheduling#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jest": "^26.4.2",
    "lint": "^0.7.0",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.25",
    "typescript": "^4.0.2"
  }
}
