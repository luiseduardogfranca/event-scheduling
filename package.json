{
  "name": "event-scheduling",
  "version": "1.0.0",
  "description": "Event scheduling API",
  "main": "index.js",
  "scripts": {
    "start": "npm run build-ts && npm run serve",
    "build": "npm run build-ts && npm run lint",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "test": "jest --runInBand --forceExit --coverage --no-cache",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "rm -rf dist && tsc",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./src/config/dbConfig.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luiseduardogfranca/event-scheduling.git"
  },
  "keywords": [
    "scheduling",
    "api",
    "event"
  ],
  "author": "Luís Eduardo Gomes França",
